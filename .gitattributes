# Auto detect text files and perform LF normalization
* text=auto
# Python files
*.py text diff=python
*.pyw text diff=python
*.pyx text diff=python
*.pxd text diff=python
# JavaScript/TypeScript files
*.js text
*.jsx text
*.ts text
*.tsx text
*.json text
*.mjs text
# Docker files
*.dockerfile text
Dockerfile text
docker-compose*.yml text
# Documentation
*.md text diff=markdown
*.mdx text diff=markdown
*.txt text
*.rst text
# Configuration files
*.yml text
*.yaml text
*.conf text
*.config text
*.ini text
*.env.example text
.env.* text
.gitignore text
.gitattributes text
# Shell scripts
*.sh text eol=lf
*.bash text eol=lf
*.zsh text eol=lf
# Windows scripts
*.bat text eol=crlf
*.cmd text eol=crlf
*.ps1 text eol=crlf
# AI Models and large files (use Git LFS)
*.onnx filter=lfs diff=lfs merge=lfs -text
*.pt filter=lfs diff=lfs merge=lfs -text
*.pth filter=lfs diff=lfs merge=lfs -text
*.h5 filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text
*.joblib filter=lfs diff=lfs merge=lfs -text
# Binary files
*.db binary
*.p binary
*.pkl binary
*.pyc binary
*.pyo binary
*.pyd binary
*.so binary
*.dylib binary
*.dll binary
*.exe binary
# Image files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.svg text
# Font files
*.ttf binary
*.eot binary
*.woff binary
*.woff2 binary
# Exclude from Git archive
.gitattributes export-ignore
.gitignore export-ignore
.gitkeep export-ignore
tests/ export-ignore
docs/ export-ignore
*.model filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
